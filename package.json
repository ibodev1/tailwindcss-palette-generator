{
  "name": "tailwindcss-palette-generator",
  "version": "2.4.0",
  "license": "MIT",
  "type": "module",
  "description": "Color palette generation library for TailwindCSS.",
  "author": "@ibodev1 <github.com/ibodev1>",
  "sideEffects": false,
  "main": "./dist/cjs/index.cjs",
  "module": "./dist/esm/index.js",
  "types": "./dist/esm/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/esm/index.d.ts",
      "import": "./dist/esm/index.js",
      "require": "./dist/cjs/index.cjs"
    },
    "./getPalette": {
      "types": "./dist/esm/getPalette.d.ts",
      "import": "./dist/esm/getPalette.js",
      "require": "./dist/cjs/getPalette.cjs"
    }
  },
  "publishConfig": {
    "access": "public"
  },
  "files": [
    "dist",
    "src",
    "LICENSE",
    "README.md"
  ],
  "scripts": {
    "build": "pnpm run clean && rslib build",
    "build:watch": "rslib build --watch",
    "test": "rstest",
    "prepublish": "pnpm run build && pnpm run test",
    "postversion": "pnpm publish --access public",
    "clean": "rimraf dist/",
    "check": "biome check .",
    "check:fix": "biome check --write .",
    "format": "biome format --write .",
    "lint": "biome lint --write ."
  },
  "engines": {
    "node": ">=20"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ibodev1/tailwindcss-palette-generator.git"
  },
  "homepage": "https://github.com/ibodev1/tailwindcss-palette-generator#readme",
  "bugs": {
    "url": "https://github.com/ibodev1/tailwindcss-palette-generator/issues"
  },
  "keywords": [
    "tailwind",
    "tailwind palette generator",
    "tailwindcss",
    "tailwindcss palette generator",
    "color palette",
    "css variables",
    "dark mode"
  ],
  "peerDependencies": {
    "tailwindcss": ">=3.4.0 || >=4.0.0 || >=0.0.0-insiders"
  },
  "devDependencies": {
    "@biomejs/biome": "^2.4.1",
    "@rslib/core": "^0.19.6",
    "@rstest/core": "^0.8.5",
    "@swc/core": "^1.15.11",
    "@tailwindcss/postcss": "^4.1.18",
    "@types/chroma-js": "^3.1.2",
    "@types/node": "^24",
    "postcss": "^8.5.6",
    "rimraf": "^6.1.3",
    "tailwindcss": "^4.1.18",
    "typescript": "^5.9.3"
  },
  "packageManager": "pnpm@10.29.3"
}
